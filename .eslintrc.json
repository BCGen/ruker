{
    "extends": ["standard", "./.vuelintrc.json"],
    "rules": {
        "import/no-extraneous-dependencies": ["error", {
            "devDependencies": true
        }],
        "no-new": 0,
        "space-before-function-paren": "off",
        "import/no-unresolved": ["error", 1]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jasmine": true
    }
}
